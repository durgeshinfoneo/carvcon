    "semantic-ui-react": "^0.79.1",
    "simpl-schema": "^1.4.2",
    "uniforms": "^1.24.0",
    "uniforms-bootstrap3": "^1.24.1",
    "uniforms-semantic": "^1.24.0",
    "validator": "^9.4.1",
