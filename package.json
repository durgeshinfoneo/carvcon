{
  "name": "carvcon",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent",
    "debug": "meteor run --inspect-brk=9229"
  },
  "eslintConfig": {
    "extends": "@meteorjs/eslint-config-meteor",
    "rules": {
      "meteor/audit-argument-checks": "off",
      "no-console": "off",
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "import/no-unresolved": "off",
      "react/forbid-prop-types": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "eslintIgnore": [
    "client/lib/*",
    "client/js/*"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "bcrypt": "^3.0.0",
    "bootstrap": "^3.3.7",
    "cheerio": "^1.0.0-rc.1",
    "classnames": "^2.2.5",
    "fast-csv": "^2.4.0",
    "fibers": "^2.0.0",
    "handlebars": "^4.0.11",
    "history": "^4.6.1",
    "iconv-lite": "^0.4.17",
    "inspect-brk": "^0.1.2",
    "intl": "^1.2.5",
    "limiter": "^1.1.2",
    "meteor-node-stubs": "^1.1.0",
    "moment": "^2.22.0",
    "nouislider": "^11.1.0",
    "owl.carousel": "^2.3.4",
    "perfect-scrollbar": "^1.3.0",
    "query-string": "^4.3.4",
    "react": "^15.6.2",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-autosuggest": "^9.3.4",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-sweetalert": "^4.3.1",
    "react-bootstrap-switch": "^15.5.3",
    "react-datetime": "^2.14.0",
    "react-dom": "^15.6.2",
    "react-ga": "^2.2.0",
    "react-helmet": "^5.2.0",
    "react-image-gallery": "^0.8.2",
